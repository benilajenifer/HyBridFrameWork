<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="My Ui Framework Design" verbose="4" thread-count="1"
	parallel="tests">
	<listeners>
		<listener
			class-name="com.qa.tutninja.listeners.ExtentReportListener" />
		<listener
			class-name="com.qa.tutninja.listeners.TestAllureListener" />
		<listener
			class-name="com.qa.tutninja.listeners.AnnotationTransformer" />
	</listeners>
	
	<test name="tutorialsNinja loginPageTest chrome_90">
		<parameter name="browser" value="chrome" />
		<parameter name="browserversion" value="90.0" />
		<classes>
			<class name="com.qa.tutninja.tests.LoginPageTest" />
		</classes>

	</test>

	<test name="tutorialsNinja AccountPageTest firefox_83">
		<parameter name="browser" value="firefox" />
		<parameter name="browserversion" value="83.0" />
		<classes>
			<class name="com.qa.tutninja.tests.AccountsPageTest" />
		</classes>

	</test>
	<test name="tutorialsNinja ProductInfoPageTest  chrome_89">
		<parameter name="browser" value="chrome" />
		<parameter name="browserversion" value="89.0" />
		<classes>
			<class name="com.qa.tutninja.tests.ProductInfoPageTest" />
		</classes>

	</test>
	<test name="tutorialsNinja RegistrationPageTest  chrome_90">
		<parameter name="browser" value="chrome" />
		<parameter name="browserversion" value="90.0" />
		<classes>
			<class name="com.qa.tutninja.tests.RegistrationPageTest" />
		</classes>

	</test>




</suite> <!-- Suite -->
